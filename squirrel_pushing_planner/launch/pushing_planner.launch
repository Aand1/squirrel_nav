<launch>
  <arg name="tolerance" default="0.05"/>
  <arg name="start_goal_frame_id" default="/map"/>
  <arg name="plan_frame_id" default="/odom"/>
  <arg name="object_frame_id" default="/base_link"/>
  <arg name="costmap_topic" default="/move_base/global_costmap/costmap"/>
  <arg name="costmap_updates_topic" default="/move_base/global_costmap/costmap_updates"/>
 
  <node pkg="squirrel_pushing_planner" type="pushing_planner_node" name="pushing_planner" output="screen" respawn="false">
    <param name="tolerance" value="$(arg tolerance)" />
    <param name="start_goal_frame_id" value="$(arg start_goal_frame_id)" />
    <param name="plan_frame_id" value="$(arg plan_frame_id)" />
    <param name="object_frame_id" value="$(arg object_frame_id)" />
    <param name="costmap_topic" value="$(arg costmap_topic)"/> 
    <param name="costmap_updates_topic" value="$(arg costmap_updates_topic)"/> 
  </node>
</launch>
