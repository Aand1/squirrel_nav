<!--Code:-->

<class_libraries>
  <library path="lib/libsquirrel_costmap_layers">
    <class type="squirrel_navigation::DownprojectionLayer" base_class_type="costmap_2d::Layer">
      <description>
	Costmap layer for kinect+laser sensors. It downprojects the obstacles on the costmap without
	adding the inflation related to any robot link.
      </description>
    </class>
    <class type="squirrel_navigation::DownprojectionMultilayer" base_class_type="costmap_2d::Layer">
      <description>
	Costmap layer for kinect+laser sensors. Support for multilayer maps, while downprojecting the 
	obstacles with different inflation radii.
      </description>
    </class>
    <class type="squirrel_navigation::MapLayer" base_class_type="costmap_2d::Layer">
      <description>
	Costmap layer for static obstacles
      </description>
    </class>
    <class type="squirrel_navigation::InflationLayer" base_class_type="costmap_2d::Layer">
      <description>
	Inflation layer for static obstacles
      </description>
    </class>
  </library>
</class_libraries>

