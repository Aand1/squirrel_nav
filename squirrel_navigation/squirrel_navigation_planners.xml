	   base_class_type="nav_core::BaseGlobalPlanner">
      <description> 
	Path planner for retracted or extended arm. 
      </description>
    </class>
    <class name="squirrel_navigation/FootprintPlanner" type="squirrel_navigation::FootprintPlanner"
    	   base_class_type="nav_core::BaseGlobalPlanner">
      <description>
    	Path planner with RRT*.
      </description>
    </class>
    <class name="squirrel_navigation/LocalPlanner" type="squirrel_navigation::LocalPlanner" 
	   base_class_type="nav_core::BaseLocalPlanner">
      <description> 
	PID controller along trajectory
      </description>
    </class>
  </library>
